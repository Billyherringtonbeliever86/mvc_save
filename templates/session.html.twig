{% extends "base.html.twig" %}
{% block body %}
    <main class="main">
        <ul>
            {% for key in sessionArray|keys %}
                <li>{{ key }} : {{ sessionArray[key] }}</li>
            {% endfor %}
        </ul>

        <a href={{path('session_delete')}}> clear session </a>
        {{ include('flash.html.twig') }}
    </main>

{% endblock %}